{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.4",
        "aprendible/storage-link-route": "^1.0",
        "barryvdh/laravel-dompdf": "^0.8.6",
        "darryldecode/cart": "^4.1",
        "doctrine/dbal": "^2.10",
        "fideloper/proxy": "^4.2",
        "fruitcake/laravel-cors": "^1.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "intervention/image": "^2.5",
        "laravel/framework": "^8.0",
        "laravel/legacy-factories": "^1.1",
        "laravel/telescope": "^4.4",
        "laravel/tinker": "^2.0",
        "laravel/ui": "^3.0",
        "laraveles/spanish": "^1.4",
        "maatwebsite/excel": "^3.1",
        "nztim/mailchimp": "^4.2",
        "rinvex/laravel-subscriptions": "^5.0",
        "spatie/laravel-permission": "^3.12",
        "spatie/opening-hours": "^2.10"
    },
    "require-dev": {
        "facade/ignition": "^2.3.6",
        "fruitcake/laravel-telescope-toolbar": "^1.3",
        "mockery/mockery": "^1.3.1",
        "nunomaduro/collision": "^5.0",
        "phpunit/phpunit": "^9.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "files": [
            "app/helpers.php"
        ],
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
